//-----------------------------------------------------------------------CO2---------------------------------------------------//
#include <SD_MHZ19B.h> // Library for the MH-Z19B CO2 sensor
#include <HardwareSerial.h>

// --- Definition of Hardware UART 2 for the ESP32 ---
HardwareSerial mhzSerial(2);  // Use ESP32's second hardware serial port

// --- Pin Definitions ---
#define MHZ19B_RX 36  // ESP32 pin connected to sensor's TX
#define MHZ19B_TX 27  // ESP32 pin connected to sensor's RX


SD_MHZ19B MHZ19B(mhzSerial); // Sensor object using Hardware Serial 2


//-----------------------------------------------------------------------NPM---------------------------------------------------//


    
     #include <Arduino.h>
    
     /*****************************************************************
      * HARDWARE CONFIGURATION
      *****************************************************************/
     
     #define serialNPM (Serial1) // Using hardware serial port 1 for the NextPM sensor
     
     
     #define PM_SERIAL_RX 39  // ESP32 pin connected to sensor's RX 
     #define PM_SERIAL_TX 32  // ESP32 pin connected to sensor's TX 
     
     /*****************************************************************
      * TIMING CONFIGURATION
      *****************************************************************/
  
     unsigned long previousMillis = 0;      // Stores the last time we read the sensor
     const long interval = 10000;           // Interval between measurement readings (10 seconds)
     
     /*****************************************************************
      * DATA STORAGE
      *****************************************************************/
     
     float pm1_ugm3 = 0.0;   // PM1.0 concentration in µg/m³
     float pm25_ugm3 = 0.0;  // PM2.5 concentration in µg/m³
     float pm10_ugm3 = 0.0;  // PM10 concentration in µg/m³
     
     /*****************************************************************
      * HELPER FUNCTIONS
      *****************************************************************/
     
     
     bool checksum_valid(const uint8_t (&data)[16]) { // Verifies if the received data frame is corrupted
         uint8_t sum = 0;
         
         for (int i = 0; i < 16; i++) {
             sum += data[i]; // Addition of all 16 bytes
         }
         
         return (sum % 0x100 == 0); // The total sum modulo 256 must be zero
     }
     
     
     void send_concentration_command() {
         const uint8_t cmd[] = {0x81, 0x12, 0x6D}; // Specific sensor command to read 1-minute average values
         serialNPM.write(cmd, 3);  // Send command through serial port 
     }
     
    // Display raw hexadecimal data in the serial monitor

     void print_data(uint8_t data[], size_t size) {
         Serial.print("Raw data: ");
         for (size_t i = 0; i < size; i++) {
             Serial.print("0x");
             if (data[i] < 0x10) Serial.print("0");  
             Serial.print(data[i], HEX);
             if (i != size - 1) Serial.print(", ");  
         }
         Serial.println();
     }
     
     /*****************************************************************
      * MAIN SENSOR READING FUNCTION
      *****************************************************************/
     
     
     void read_concentration() {
         Serial.println("\n--- Reading NextPM Concentrations ---");
     
         // Step 1: Send command to the sensor 
         send_concentration_command();
     
         // Step 2: Wait for response with a 3-second timeout
         // Sensor typically responds in 1s, but we allow up to 3s
         unsigned long timeout = millis();
         while (!serialNPM.available() && millis() - timeout < 3000) {
             delay(10);
         }
     
         // Check if sensor responded
         if (!serialNPM.available()) {
             Serial.println("ERROR: No response from sensor");
             return;
         }
     
         // Step 3: Search for message header: 0x81 0x12
         // This header identifies a concentration data frame
         const uint8_t header[2] = {0x81, 0x12};
         if (!serialNPM.find(header, 2)) {
             Serial.println("ERROR: Header not found");
             return;
         }
     
         // Step 4: Read the sensor status byte
         // This byte contains the sensor's health/operational state 
         uint8_t state;
         if (serialNPM.readBytes(&state, 1) != 1) {
             Serial.println("ERROR: Failed to read state");
             return;
         }
     
         // Step 5: Read the 12 data bytes
         // These bytes contain 6 values (2 bytes each): N1, N2.5, N10, PM1, PM2.5, PM10
         uint8_t data[12];
         if (serialNPM.readBytes(data, 12) != 12) {
             Serial.println("ERROR: Failed to read data");
             return;
         }
     
         // Step 6: Read the checksum byte
         // Used to verify data integrity
         uint8_t checksum;
         if (serialNPM.readBytes(&checksum, 1) != 1) {
             Serial.println("ERROR: Failed to read checksum");
             return;
         }
     
         // Step 7: Reconstruct the full 16-byte message for validation
         uint8_t full_msg[16];
         full_msg[0] = header[0];      // 0x81
         full_msg[1] = header[1];      // 0x12
         full_msg[2] = state;          // State byte
         memcpy(&full_msg[3], data, 12);  // Copy the 12 data bytes
         full_msg[15] = checksum;      // Checksum byte 
     
         // Step 8: Validate the checksum
         if (!checksum_valid(full_msg)) {
             Serial.println("ERROR: Invalid checksum");
             print_data(full_msg, 16);  // Print raw data for debugging
             return;
         }
     
         // Step 9: Parse the received concentration data

         // Define a 16-bit unsigned integer to store raw PM1.0 value
         // word(data[6], data[7]) combines the High byte and Low byte into a single number
         uint16_t pm1_raw = word(data[6], data[7]);
     
         // Same operation for PM2.5: retrieving bytes 8 and 9 from the data array
         uint16_t pm25_raw = word(data[8], data[9]);
     
         // Same operation for PM10: retrieving the last two data bytes (10 and 11)
         uint16_t pm10_raw = word(data[10], data[11]);
     
         // Step 10: Convert raw values into actual concentrations
         // Divide by 10.0 to handle the decimal precision (float calculation)
         pm1_ugm3 = pm1_raw / 10.0;
         pm25_ugm3 = pm25_raw / 10.0;
         pm10_ugm3 = pm10_raw / 10.0;
     
         // Step 11: Display results in the serial monitor
         Serial.println("\n=== Particulate Matter Concentrations ===");
     
         Serial.print("PM1.0:  ");
         Serial.print(pm1_ugm3, 1);  // Print with 1 decimal place
         Serial.println(" µg/m³");
     
         Serial.print("PM2.5:  ");
         Serial.print(pm25_ugm3, 1);
         Serial.println(" µg/m³");
     
         Serial.print("PM10:   ");
         Serial.print(pm10_ugm3, 1);
         Serial.println(" µg/m³");
     
         Serial.println("======================================\n");
     }
  
  

//-----------------------------------------------------------------------Screen---------------------------------------------------//


// Pin definitions for the PxMatrix screen 
// Connection is established via HUB75 (GPIO) using a 16-pin connector

#define P_LAT 25
#define P_A 17
#define P_B 33
#define P_C 4
#define P_D 12
#define P_E 15
#define P_OE 16

hw_timer_t * timer = NULL; // Internal ESP32 timer for smooth display refresh 
portMUX_TYPE timerMux = portMUX_INITIALIZER_UNLOCKED; // Mux variable to prevent concurrent screen access

// Screen dimensions in pixels
#define matrix_width 64 // Width 
#define matrix_height 32 // Height

uint8_t display_draw_time = 30; // LED illumination time in microseconds
PxMATRIX display(64, 32, P_LAT, P_OE, P_A, P_B, P_C, P_D, P_E); // Initialize display object with pins and dimensions

// Refresh function stored in IRAM (RAM) for speed
void IRAM_ATTR display_updater()
{
  // Enter critical section so ESP32 doesn't interrupt this command 
  portENTER_CRITICAL_ISR(&timerMux); 
  display.display(display_draw_time);// Physically refresh pixel data on the screen
  portEXIT_CRITICAL_ISR(&timerMux); // Exit critical section to resume other tasks
}

// Function to enable/configure the refresh timer
// Ensures constant background display while multitasking
void display_update_enable(bool is_enable) 
{
  Serial.print("Call display_update_enable function with:");
  if (is_enable)
  {
    Serial.println("true");
    timerAttachInterrupt(timer, &display_updater, true);
    timerAlarmWrite(timer, 4000, true);// Set alarm to trigger every 4000 microseconds
    timerAlarmEnable(timer);// Start the timer
  }
  else
  {
    Serial.println("false");// Disable timer and detach interrupt
    timerDetachInterrupt(timer);
    timerAlarmDisable(timer);
  }
}

