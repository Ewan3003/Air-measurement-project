  0x0000,0x0000,0x0000,0x0000,0x0000,0x0000,0x0000,0x0000,0x0000,0x0000,0x0000,0x0000,0x0000,0x0000,0x0000,0x0000,
  0x0000,0x0000,0x0000,0x0000,0x0000,0x0000,0x0000,0x0000,0x0000,0x0000,0x0000,0x0000,0x0000,0x0000,0x0000,0x0000,
  0x0000,0x0000,0x0000,0x0000,0x0000,0x0000,0x0000,0x0000,0x0000,0x0000,0x0000,0x0000,0x0000,0x0000,0x0000,0x0000  
};



// RGB to RBG color correction
uint16_t convertRGBtoRBG(uint16_t color) {
  // Extract RGB565 components
  uint8_t r = (color >> 11) & 0x1F;// Extract 5 bits for Red by shifting 11 bits right
  uint8_t g = (color >> 5)  & 0x3F;// Extract 6 bits for Green by shifting 5 bits
  uint8_t b = (color)       & 0x1F;// Extract 5 bits for Blue

  // Reconstruct color: Red stays, but Blue and Green channels are swapped
  // Blue is shifted to the Green position (<< 5), and Green takes the Blue position.
  return (r << 11) | (b << 5) | g;
}

// Function to apply correction to a whole image (pixel array)
void fixImage(uint16_t *img, int w, int h) { 
  for (int i = 0; i < w * h; i++) { // Loop through every pixel (width * height)
    img[i] = convertRGBtoRBG(img[i]); // Replace original color with the corrected RBG version
  }
}

float x;// Variable for horizontal position (used for WiFi icon placement)
float y;// Variable for vertical position

unsigned long lastChange = 0;// Stores timestamp (ms) of the last image switch
int currentImage = 0;// Index for the currently displayed metric (0: PM10, 1: PM2.5, etc.)

// Returns a color (Green, Orange, or Red) based on value and defined thresholds
uint16_t getCouleurSeuil(float valeur, float seuilVert, float seuilOrange) {
  if (valeur <= seuilVert) {
    return display.color565(0, 0, 255);       // Green (RBG mapping)
  } else if (valeur <= seuilOrange) {
    return display.color565(255, 0, 150);     // Orange
  } else {
    return display.color565(255, 0, 0);       // Red
  }
}



void setup() {
  
  Serial.begin(115200);
  //-----------------------------------------------------------------------CO2---------------------------------------------------//

  // Initialize UART communication for the CO2 sensor
  mhzSerial.begin(9600, SERIAL_8N1, MHZ19B_RX, MHZ19B_TX);

  // Sensor configuration
  MHZ19B.setAutoCalibration(false);  // Disable automatic calibration
  MHZ19B.setDetectionRange(2000);    // Set detection range to 0–2000 ppm

  //-----------------------------------------------------------------------WIFI---------------------------------------------------//
  // ---------- EDUROAM CONNECTION ----------
  
  Serial.println("Connecting to eduroam...");

  WiFi.disconnect(true);
  WiFi.mode(WIFI_STA);
  delay(500);

  // WPA2 Enterprise (PEAP) connection setup
  esp_wifi_sta_wpa2_ent_set_identity((uint8_t*)EAP_IDENTITY, strlen(EAP_IDENTITY));
  esp_wifi_sta_wpa2_ent_set_username((uint8_t*)EAP_USERNAME, strlen(EAP_USERNAME));
  esp_wifi_sta_wpa2_ent_set_password((uint8_t*)EAP_PASSWORD, strlen(EAP_PASSWORD));
 // Enable WPA2 Enterprise mode
 esp_wifi_sta_wpa2_ent_enable();

  WiFi.begin(ssid);

  int timeout = 0;
  while (WiFi.status() != WL_CONNECTED) {
    Serial.print(".");
    delay(300);
    timeout++;
    if (timeout > 100) ESP.restart(); // Restart ESP if connection fails after several attempts
  }
  Serial.println("\nConnected to eduroam!");
  Serial.print("IP Address: ");
  Serial.println(WiFi.localIP());

  // ---------- NTP TIME SYNC ----------
  timeSync(TZ_INFO, "pool.ntp.org", "time.nis.gov");

  // ---------- INFLUXDB CONNECTION TEST ----------
  if (client.validateConnection()) {
    Serial.print("Connected to InfluxDB: ");
    Serial.println(client.getServerUrl());
  } else {
    Serial.print("InfluxDB Error: ");
    Serial.println(client.getLastErrorMessage());
  }

  // Set tags for identification
  sensor.addTag("device", DEVICE);
  sensor.addTag("SSID", WiFi.SSID());

  //-----------------------------------------------------------------------Screen---------------------------------------------------//
  
  display.begin(16);// Screen refreshes rows in blocks of 16 for better synchronization
  // Enable software optimization for faster data transmission to the matrix
 // Frees up CPU time for sensor processing and WiFi tasks
  display.setFastUpdate(true);

  // Timer initialization
  timer = timerBegin(0, 80, true);  // Timer 0, prescaler 80, count up
  timerAlarmWrite(timer, 4000, true);  // Interrupt triggered every 4ms
  
  // Enable display refresh
  display_update_enable(true);

x = 52 ; // Image position (width)
y = -7; // Image position (height)

// Apply color correction to all images
fixImage(wifi1, 16, 16);
fixImage(wifi2, 16, 16);
fixImage(wifi3, 16, 16);
fixImage(wifi4, 16, 16);
fixImage(logo, x, y);
fixImage(PM10, x, y);
  
//-----------------------------------------------------------------------NPM---------------------------------------------------//
    

// Initialize serial communication for the NPM sensor
serialNPM.begin(115200, SERIAL_8E1, PM_SERIAL_RX, PM_SERIAL_TX);

// Set serial timeout to 3 seconds 
// Measurement operation fails if sensor does not respond within this window
serialNPM.setTimeout(3000);

// Sensor Wake-up
// NextPM requires 15 seconds to initialize properly 
// Reading data too early leads to empty or invalid measurements

display.fillScreen(0); // Clear screen and show O'Air logo while sensor warms up
display.drawRGBBitmap(0, 0, logo, 64, 32); // Center the logo 
delay(15000);
   
}

void loop() {
  // put your main code here, to run repeatedly:

  //-----------------------------------------------------------------------CO2---------------------------------------------------//
uint16_t co2_ppm = MHZ19B.getPPM(); // Measure CO2 concentration in PPM
  int temperature_c = MHZ19B.getTemperature(); // Get sensor temperature (to detect overheating)
  int status = MHZ19B.getStatus(); // Check sensor health status

  //-----------------------------------------------------------------------NPM---------------------------------------------------//
// Get current time elapsed since startup in milliseconds
       unsigned long currentMillis = millis();
   
       // Ensure enough time has passed before taking the next measurement
       // 10-second interval between each sensor reading
       if (currentMillis - previousMillis >= interval) {
           // Save current time as the new reference for the next 10s cycle
           previousMillis = currentMillis;
   
           read_concentration(); // Execute concentration reading logic
       }

  //-----------------------------------------------------------------------WIFI---------------------------------------------------//
  // WiFi management and InfluxDB data transmission 
  sensor.clearFields();
  sensor.addField("rssi", WiFi.RSSI()); // WiFi signal strength (RSSI)
  sensor.addField("CO2" , co2_ppm); // CO2 value
  sensor.addField("PM1", pm1_ugm3); // PM1 value
  sensor.addField("PM25", pm25_ugm3); // PM2.5 value
  sensor.addField("PM10", pm10_ugm3); // PM10 value
  sensor.addField("rssi", WiFi.RSSI());

  Serial.print("Writing: ");
  Serial.println(sensor.toLineProtocol());

  if (WiFi.status() != WL_CONNECTED) {
    Serial.println("WiFi lost! Attempting to reconnect..."); // Attempt reconnection if WiFi drops
  }

  if (!client.writePoint(sensor)) {
    Serial.print("InfluxDB write error: ");
    Serial.println(client.getLastErrorMessage()); // Display error message if database sync fails
  }


  //-----------------------------------------------------------------------Screen---------------------------------------------------//
  Serial.println("Showing message on LED matrix");
  
  // Clear screen at the start of each cycle to prevent text ghosting
  display.fillScreen(0); 
  display.setTextSize(1);

  // Cycle through metrics every 5 seconds (CO2, PM1, PM2.5, PM10)
  if (millis() - lastChange > 5000) {
    lastChange = millis();
    currentImage++;
    if (currentImage > 3) currentImage = 0;
  }

  // Variable to store dynamic text color
  uint16_t couleurTexte;

  // Display specific icon and measurement based on current cycle
  switch (currentImage) {
    case 0: // PM 10
      display.drawRGBBitmap(0, -5, PM10, 64, 32);
      
      // PM10 Thresholds: Green(0-40), Orange(40-100), Red(>100)
      couleurTexte = getCouleurSeuil(pm10_ugm3, 40, 100);
      display.setTextColor(couleurTexte);
      
      display.setCursor(32, 16);
      display.print(pm10_ugm3, 1);
      break;

    case 1: // PM 2.5
      display.drawRGBBitmap(0, -5, pm25, 64, 32);
      
      // PM2.5 Thresholds: Green(0-20), Orange(20-50), Red(>50)
      couleurTexte = getCouleurSeuil(pm25_ugm3, 20, 50);
      display.setTextColor(couleurTexte);
      
      display.setCursor(32, 16);
      display.print(pm25_ugm3, 1);
      break;

    case 2: // PM 1.0
      display.drawRGBBitmap(0, -5, pm1, 64, 32);
      
      // PM1 Thresholds: Green(0-15), Orange(15-37), Red(>37)
      couleurTexte = getCouleurSeuil(pm1_ugm3, 15, 37);
      display.setTextColor(couleurTexte);
      
      display.setCursor(31, 16);
      display.print(pm1_ugm3, 1);
      break;

    case 3: // CO2
      display.drawRGBBitmap(0, -5, co2, 64, 32);
      
      // CO2 Thresholds: Green(0-800), Orange(800-1500), Red(>1500)
      couleurTexte = getCouleurSeuil(co2_ppm, 800, 1500);
      display.setTextColor(couleurTexte);
      
      display.setCursor(30, 16);
      display.print(co2_ppm, 1);
      break;
  }

  // WiFi Overlay - Displayed permanently in the top corner based on RSSI strength
  if (WiFi.RSSI() > -50) {
    display.drawRGBBitmap(x, y, wifi4, 16, 16);
  }
  else if (WiFi.RSSI() > -70) {
    display.drawRGBBitmap(x, y, wifi3, 16, 16);
  }
  else if (WiFi.RSSI() > -80) {
    display.drawRGBBitmap(x, y, wifi2, 16, 16);
  }
  else {
    display.drawRGBBitmap(x, y, wifi1, 16, 16);
  }

  delay(20); // Short delay for loop stability

}
